<query id="updatePlanetTwitter" action="update">
    <tables>
        <table name="planet" />
    </tables>
    <columns>
        <column name="twitter_uid" var="twitter_uid" minlength="1" maxlength="250" />
        <column name="twitter_authkey" var="twitter_authkey" minlength="1" maxlength="250" />
        <column name="twitter_autopush" var="twitter_autopush" default="N" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull"/>
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" pipe="and" />
    </conditions>
</query>
