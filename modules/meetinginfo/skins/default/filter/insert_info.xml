<filter name="insert_info" module="meetinginfo" act="procMeetinginfoAdd" confirm_msg_code="confirm_submit">
    <form>
        <node target="host_srl" required="true" minlength="1" />
        <node target="guest_srl" required="true" minlength="1" />
        <node target="sender_log" minlength="1" />
        <node target="content" required="true" minlength="1" />
    </form>
    <parameter />
    <response callback_func="completeAddInfo">
        <tag name="error" />
        <tag name="message" />
        <tag name="message_type" />
		<tag name="page" />
	</response>
</filter>
